# fish-ai configuration template
# Copy this file to fish-ai.ini and fill in your API key
# See https://github.com/Realiserad/fish-ai for more configuration options

[fish-ai]
configuration = anthropic

[anthropic]
provider = anthropic
api_key = your-anthropic-api-key-here

# Optional settings:
# language = English
# temperature = 0.2
# completions = 5
# refined_completions = 3
# history_size = 0
# preview_pipe = False
# debug = False